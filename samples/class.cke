class Person {
  prop firstName
  prop lastName

  fun init(firstName, lastName) {
    this.firstName = firstName
    this.lastName = lastName
  }

  fun getFirstName() { this.firstName }
  fun getLastName() { this.lastName }
}

let person = Person("Denis", "Defreyne")

print(person.getFirstName())
print(person.getLastName())

let getFirstName = person.getFirstName
print(getFirstName())

print(person.firstName)
print(person.lastName)

print(Person)
print(person)
