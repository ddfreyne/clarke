class Person {
  ivar firstName: string
  ivar lastName: string

  fun init(firstName: string, lastName: string) {
    @firstName = firstName
    @lastName = lastName
  }

  fun firstName() { @firstName }
  fun lastName() { @lastName }
}

let person = Person("Denis", "Defreyne")

print(person.firstName())
print(person.lastName())

let firstName = person.firstName
print(firstName())

print(person.firstName)
print(person.lastName)

print(Person)
print(person)
