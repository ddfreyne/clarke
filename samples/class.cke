class Person {
  fun init(firstName, lastName) {
    this._firstName = firstName
    this._lastName = lastName
  }

  fun firstName() { this._firstName }
  fun lastName() { this._lastName }
}

let person = Person("Denis", "Defreyne")

print(person.firstName())
print(person.lastName())

let firstName = person.firstName
print(firstName())

print(person.firstName)
print(person.lastName)

print(Person)
print(person)
